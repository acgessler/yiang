
build optimized {
	group re ^[0-9]+\.txt$
		push archive_files ~
	end
}


push files (expand 60000.txt)

group re ^[0-9]+$
	group type dir 
		build ~ {
			build cooked {
				push archive_files (expand tiles.dat)
			}
			group type file
				push archive_files ~
			end
		}
	end
end



group unpushed 
	group re ^[0-9]+\.txt$ 
		group type file
			push archive_files ~
		end
	end
end

	
